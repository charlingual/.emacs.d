(setq load-path (cons "~/.emacs.d/lisp" load-path))
(setq load-path
      (append
      (list
       (expand-file-name "~/.emacs.d/lisp/")
       )
       load-path))



(set-language-environment 'Japanese)
(prefer-coding-system 'utf-8)
(setq load-path (cons "~/.emacs.d/lisp" load-path))

(add-to-list 'load-path "~/.emacs.d/lisp/")
